    "gulp": "^3.9.1",
    "gulp-sass": "^2.2.0",
    "gulp-typescript": "^2.11.0",
    "material-design-icons": "^2.2.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp-rename": "^1.2.2",
